<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="org.apache.dolphinscheduler.dao.mapper.CommandPushMapper">
    <sql id="baseSql">
        id, command_type, process_definition_code, process_definition_version, process_instance_id, command_param, task_depend_type, failure_strategy, warning_type, 
        warning_group_id, schedule_time, start_time, executor_id, update_time, process_instance_priority, worker_group, environment_code, dry_run, dep_data_names, dep_data_time_replaced_name, online_flag
    </sql>
    
    <select id="queryCommandPage" resultType="org.apache.dolphinscheduler.dao.entity.CommandPush">
        select *
        from t_ds_command_push
        order by process_instance_priority, id asc
        limit #{limit} offset #{offset}
    </select>
    
    <select id="queryByProcessDefinitionCode" resultType="org.apache.dolphinscheduler.dao.entity.CommandPush">
        select
		<include refid="baseSql"/>
        from t_ds_command_push
        where process_definition_code = #{processDefinitionCode}
    </select>
    
</mapper>
